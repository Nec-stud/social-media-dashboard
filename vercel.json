{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/auth/twitter",
        "dest": "/server.js"
      },
      {
        "src": "/auth/callback",
        "dest": "/server.js"
      }
    ]
  }